<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:background="@drawable/bgrnd"
    android:orientation="vertical"
    tools:context=".Faculty.Home">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:background="#F5F5F5">
        <ImageButton
            android:id="@+id/adddocument"
            android:layout_width="150dp"
            android:layout_height="101dp"
            android:background="@drawable/addoc" />


        <ImageButton

            android:id="@+id/alumni1"
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:background="@drawable/alumni" />






    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageButton
            android:id="@+id/calendar1"
            android:layout_width="150dp"
            android:layout_height="101dp"
            android:background="@drawable/calendar" />

        <ImageButton
            android:id="@+id/notice1"
            android:layout_width="150dp"
            android:layout_height="101dp"
            android:background="@drawable/notice" />



    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageButton
            android:id="@+id/profile1"
            android:layout_width="150dp"
            android:layout_height="101dp"
            android:background="@drawable/info" />
        <ImageButton
            android:id="@+id/resume1"
            android:layout_width="150dp"
            android:layout_height="101dp"
            android:background="@drawable/resume" />

    </LinearLayout>




</LinearLayout>